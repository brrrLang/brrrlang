@pkg "Main";

@import "Std";

@use Std::IO::[PrintLine, ReadLine];

() {
    PrintLine("Hello, what is your name? ");
    ReadLine() -> new name;
    PrintLine(StrFmt("Hello {}, how old are you? ", name));
    example_project::Example::ReadI32 -> new age;
    PrintLine(StrFmt("Hello {}, you are {} years old", name, age));
} -> Main