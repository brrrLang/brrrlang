@pkg "Example";

@import "Std";

@use Std::IO::ReadLine;
@use Std::IO::PrintLine;
@use Std::IO::StdErr;
@use Std::String::StrParse;
@use Std::String::StrFmt;

() {
    loop {
        ReadLine() -> new val;
        StrParse<i32>(val)
            | Ok(a) return a
            | Err(_) PrintLine(StdErr, StrFmt("{} is not a number", val))
    }
} -> pub ReadI32<i32>
